package this_is_java.chap08;

public class SoundableExample {
	private static void printSound(Soundable soundable) {
		System.out.println(soundable.sound());
		
	}
	
	public static void main(String[] args) {
		Soundable soundable;
		soundable = new Cat();
		soundable = new Dog();
		printSound(new Cat());
		printSound(new Dog());
		
	}

}
